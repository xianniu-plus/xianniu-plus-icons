{
  "name": "@xianniu-plus/icons-vue",
  "version": "2.3.2",
  "description": "Vue components of Xianniu Plus Icons collection.",
  "type": "module",
  "keywords": [
    "icon",
    "svg",
    "vue",
    "xianniu-plus"
  ],
  "license": "MIT",
  "homepage": "https://xianniu-plus.org/",
  "bugs": {
    "url": "https://github.com/xianniu-plus/xianniu-plus-icons/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xianniu-plus/xianniu-plus-icons.git",
    "directory": "packages/vue"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./global": {
      "types": "./dist/types/global.d.ts",
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/types/*"
      ]
    }
  },
  "sideEffects": false,
  "unpkg": "dist/index.iife.min.js",
  "jsdelivr": "dist/index.iife.min.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "scope": "@xianniu-plus"
  },
  "scripts": {
    "build": "pnpm run build:generate && run-p build:build build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "NODE_ENV=production tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@pnpm/find-workspace-dir": "^6.0.3",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/logger": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.25",
    "@xianniu-plus/icons-svg": "workspace:*",
    "camelcase": "^8.0.0",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "esbuild": "^0.20.1",
    "esbuild-plugin-globals": "^0.2.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "unplugin-vue": "^5.0.4",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  }
}
